							li
								+icon("check",16,16)
								p Watts
							li
								+icon("check",16,16)
								p Watt Demand
							li
								+icon("check",16,16)
								p Va
							li
								+icon("check",16,16)
								p Va Demand
							li
								+icon("check",16,16)
								p Vars
							li
								+icon("check",16,16)
								p Var Demand
							li
								+icon("check",16,16)
								p Demand Metering
							li
								+icon("check",16,16)
								p Frequency
							li
								+icon("check",16,16)
								p Apparent Power Factor
					.item__specifications 
						span Logging & Alarms
						ul.specifications-list
							li
								+icon("check",16,16)
								p Min/Max Log
					.item__specifications 
						span Communications
						ul.specifications-list
							li
								+icon("check",16,16)
								p 10/100 RJ-45 Ethernet Port  (option)
							li
								+icon("check",16,16)
								p Modbus RTU (RS-485)  (option)
							li
								+icon("check",16,16)
								p Modbus TCP (option)
							li
								+icon("check",16,16)
								p Fully Programmable wo/ dedicated software
					.item__specifications 
						span Operational Specifications
						ul.specifications-list
							li
								+icon("check",16,16)
								p +/- % Accuracy --Current:
									span '0.25 Reading
							li
								+icon("check",16,16)
								p +/- % Accuracy --Voltage:
									span 0.25 Reading
							li
								+icon("check",16,16)
								p Meets Ansi % Revenue Accuracy:
									span C12.20(0.5)
							li
								+icon("check",16,16)
								p Meets IEC % Revenue Accuracy:
									span 687(0.5)
							li
								+icon("check",16,16)
								p Operating Temp. Range --Degrees C:
									span '-20 to 70
							li
								+icon("check",16,16)
								p Number of Current Inputs:
									span 3
							li
								+icon("check",16,16)
								p No PTs Below 600 Vac:
									span yes
							li
								+icon("check",16,16)
								p No PTs For Floating 3 Wire (Delta) < 600 Vac:
									span yes
